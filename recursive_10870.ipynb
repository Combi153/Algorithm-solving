   "source": [
    "def fibo(num):\n",
    "    if num <=1:\n",
    "        return num\n",
    "    return fibo(num-1)+fibo(num-2)\n",
    "\n",
    "n=int(input())\n",
    "print(fibo(n))"
   ]
